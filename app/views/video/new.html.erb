<br>
<table>
  <tr>
    <td>Class</td>
    <td><%= @batch.name %></td>
  </tr>
  <tr>
    <td>Subject</td>
    <td><%= @subject.subject %></td>
  </tr>
  <tr>
    <td>Staff</td>
    <td><%= @teacher.fname %> <%= @teacher.lname %></td>
  </tr>
  <tr>
    <td>Title</td>
    <td><%= @chapter.title %></td>
  </tr>
  <tr>
    <td>Overview</td>
    <td><%= @chapter.overview %></td>
  </tr>
</table>
<br>
<%= form_for @video do |f| %>
  <table>
    <tr>
      <td><%= f.label :title %></td>
      <td><%= f.text_field :title, id: :video_title %></td>
    </tr>
    <tr>
      <td><%= f.label :file %></td>
      <td><%= f.file_field :file, id: :video_file %></td>
    </tr>
    <tr>
      <td><%= f.submit %></td>
    </tr>
  </table>
<% end %>
