<%= form_for @batch do |f| %>

  <div class="field">
    <%= f.label :batch %><br />
    <%= f.select :batch, [['Batch',nil],['LKG',-1],['UKG',0],['I',1],['II',2],['III',3],
    ['IV',4],['V',5],['VI',6],['VII',7],['VIII',8],['IX',9],['X',10],['XI',11],['XII',12],] %>
  </div>

  <div class="field">
    <%= f.label :section %><br />
    <%= f.text_field :section, style: "text-transform:uppercase"%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
